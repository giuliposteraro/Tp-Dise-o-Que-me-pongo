{{#partial "contenido"}}
<p>Este es tu calendario, <b>{{username}}</b></p>

<div class="container">
  <h2>Eventos</h2>
  <a class="btn btn-primary" href="/calendar/new" role="button">Agregar evento</a>
  <ul class="list-group">
    {{#each eventos}}
    <li class="list-group-item">{{motivo}}</li>
    {{/each}}
  </ul>
</div>

{{/partial}}
{{> layout.hbs}}
